<?xml version='1.1' encoding='UTF-8'?>
<project>
  <description>A simple test job created by Ansible to validate API-based triggers with a PAT.</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <builders>
    <hudson.tasks.Shell>
      <command>
        echo "=========================================="
        echo "Job triggered successfully via the build API!"
        echo "This confirms the PAT is working."
        echo "=========================================="
      </command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>